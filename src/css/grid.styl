#wrapper
	width 100%
	height 100%
.raw
	width 100%
	column_style()
	box-sizing border-box
	clear left 

.container
	margin 0 auto
	position relative
	box-sizing border-box
	display table
.container-full
	margin 0 auto
	position relative
	box-sizing border-box
	display table
	width 100%

.container 
	width: 1170px
		
for num in 1..12
	.col-{num}
		float left

